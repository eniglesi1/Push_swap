{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc compilar archivo activo",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-fsanitize=address",
                "/workspaces/Push_swap/Parseo.c",
                "/workspaces/Push_swap/Movements.c",
                "/workspaces/Push_swap/Utils.c",
                "/workspaces/Push_swap/Push_swap.c",
                "-I/workspaces/Push_swap",
                "-I/workspaces/Push_swap/printf",
                "-L/workspaces/Push_swap/printf",
                "/workspaces/Push_swap/printf/*.c",
                "/workspaces/Push_swap/printf/libft/*.c",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tarea generada por el depurador."
        }
    ],
    "version": "2.0.0"
}